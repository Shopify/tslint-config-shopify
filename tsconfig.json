{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["es6"],
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "./build",
        "noEmitOnError": true,
        "moduleResolution": "node"
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./node_modules",
        "./build",
        "./test/rules/**/*",
        "./scripts"
     ]
}
